<h2>Login</h2>
<form action="/login" method="post">
	<fieldset>
		<legend>Login</legend>
		<input type="text" name="login" size="26" id="login" placeholder="Username">
		<input type="password" name="password" size="20" id="password" placeholder="Password">
		<button name="commit" value="Login" type="submit">Login</button>
	</fieldset>
</form>